{
  "name": "mmb-electron",
  "version": "3.1.1",
  "author": "IMFS",
  "description": "An electron-vue project",
  "license": "GPL-3.0-or-later",
  "main": "./dist/electron/main.js",
  "engines": {
    "node": "^8.0.0"
  },
  "scripts": {
    "build": "npm run test && node .electron-vue/build.js && electron-builder",
    "build:windows": "npm run build && node .scripts/download-deps.js && electron-builder -c electron-builder-aio.yml",
    "build:dir": "npm run test && node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:ocp": "npm run build:web && cp -r dist/web/* server/public/",
    "build:web": "npm run test && cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "npm run lint:fix",
    "deploy": "node .scripts/deploy.sh",
    "test": "cross-env NODE_ENV=test mocha"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.2",
    "@sentry/browser": "^4.6.6",
    "@sentry/electron": "^1.1.0-beta.0",
    "ajv": "^6.10.2",
    "axios": "^0.18.1",
    "bootstrap-scss": "^4.1.3",
    "bootstrap-vue": "^2.1.0",
    "chokidar": "^3.3.0",
    "cross-spawn": "^6.0.5",
    "deepmerge": "^2.2.1",
    "electron-store": "^2.0.0",
    "electron-window-state": "^5.0.3",
    "file-saver": "^2.0.2",
    "font-awesome": "^4.7.0",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "highcharts": "^7.2.2",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.pick": "^4.4.0",
    "memoize-one": "^5.1.1",
    "monaco-editor": "^0.17.1",
    "monaco-editor-webpack-plugin": "^1.7.0",
