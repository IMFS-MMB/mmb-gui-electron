{
  "name": "mmb-electron",
  "version": "0.0.1",
  "author": "IMFS",
  "description": "An electron-vue project",
  "license": null,
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "build:server": "npm run build:web && cp -r dist/web/* server/public/",
    "dev": "node .electron-vue/dev-runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "npm run lint:fix",
    "deploy": "node .scripts/deploy.sh"
  },
  "build": {
    "productName": "mmb-electron",
    "appId": "com.macromodelbase.mmci",
    "asar": false,
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/mac/icon.icns"
    },
    "win": {
      "icon": "build/icons/win/icon.ico"
    },
    "linux": {
      "icon": "build/icons/png"
    }
  },
  "dependencies": {
    "@feathersjs/feathers": "^3.3.0",
    "@feathersjs/rest-client": "^1.4.6",
    "@sentry/browser": "^4.4.2",
    "@sentry/electron": "^0.14.0",
    "axios": "^0.18.0",
    "bootstrap-scss": "^4.1.3",
    "bootstrap-vue": "^2.0.0-rc.11",
    "command-exists": "^1.2.8",
    "cross-spawn": "^6.0.5",
    "deepclone": "^1.0.2",
    "deepmerge": "^2.2.1",
    "electron-settings": "^3.2.0",
    "electron-store": "^2.0.0",
    "electron-window-state": "^5.0.3",
    "fast-deep-equal": "^2.0.1",
    "feathers-vuex": "^1.6.2",
    "file-saver": "^2.0.0",
    "font-awesome": "^4.7.0",
    "glob": "^7.1.3",
    "highcharts": "^6.2.0",
    "jquery": "^3.3.1",
    "lodash.foreach": "^4.5.0",
    "lodash.groupby": "^4.6.0",
    "lodash.map": "^4.6.0",
    "lodash.pick": "^4.4.0",
    "papaparse": "^4.6.3",
    "vue": "^2.5.21",
    "vue-chat-scroll": "^1.3.5",
    "vue-electron": "^1.0.6",
    "vue-router": "^3.0.2",
    "vue-slider-component": "^2.8.6",
    "vuex": "^3.0.1",
    "vuex-electron": "^1.0.2",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "ajv": "^6.6.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^2.1.2",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^4.0.3",
    "electron-builder": "^20.38.4",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.12.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "0.4.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.0.3",
    "ssh2-sftp-client": "^2.4.3",
    "style-loader": "^0.21.0",
    "terser": "3.14.1",
    "url-loader": "^1.0.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.28.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.14",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.5"
  }
}
