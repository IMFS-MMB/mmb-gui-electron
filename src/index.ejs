<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Macroeconomic Model Database</title>
    <link rel="icon" href="static/favicon.ico" type="image/x-icon">
    <% if (htmlWebpackPlugin.options.nodeModules) { %>
      <!-- Add `node_modules/` to global paths so `require` works properly in development -->
      <script>
        require('module').globalPaths.push('<%= htmlWebpackPlugin.options.nodeModules.replace(/\\/g, '\\\\') %>')
      </script>
    <% } %>
  </head>
  <body <% if (process.env.IS_WEB) {%> style="background: url('//www.macromodelbase.com/files/background%20image/background.jpg')" <%}%>>
    <div id="app"></div>

    <script>
      window.__buildtime = "<%= process.env.NODE_ENV !== 'development' ? new Date(Date.now()).toLocaleString('en', { timeZone: 'UTC' }) + ' UTC' : 'Development build' %>";
    </script>

    <!-- Set `__static` path to static files in production -->
    <% if (!process.env.IS_WEB) { %>
      <script>
        if (process.env.NODE_ENV !== 'development') window.__static = require('path').join(__dirname, '/static').replace(/\\/g, '\\\\')
      </script>
    <% } %>

    <!-- webpack builds are automatically injected -->
  </body>
</html>
